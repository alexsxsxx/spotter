{
  "name": "spotter",
  "version": "1.0.7",
  "description": "Productivity tool that allows to launch external application actions and applications themselves. Supports plugins, cross platform, written without electron.",
  "keywords": [
    "Spotter",
    "launcher",
    "productivity",
    "spotlight"
  ],
  "private": true,
  "scripts": {
    "start": "npm run start:macos",
    "test": "jest",
    "lint": "eslint .",
    "install:macos": "cd ./macos && pod install",
    "start:macos": "node node_modules/react-native-macos/local-cli/cli.js run-macos",
    "build:macos": "node node_modules/react-native-macos/local-cli/cli.js run-macos --configuration Release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@spotter-app/core": "^1.0.13",
    "@types/math-expression-evaluator": "^1.2.0",
    "@types/mathjs": "^6.0.11",
    "buffer": "^6.0.3",
    "mathjs": "^9.2.0",
    "node-fetch": "^2.6.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-macos": "0.63.27",
    "react-native-sqlite-2": "^3.1.1",
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^16.9.49",
    "@types/react-native": "^0.63.15",
    "@types/react-test-renderer": "^16.9.3",
    "@types/spotify-web-api-node": "^5.0.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
