{
  "name": "spotter",
  "version": "0.0.4",
  "description": "Productivity tool that allows to launch external application actions and applications themselves. Supports plugins, cross platform, written without electron.",
  "keywords": [
    "Spotter",
    "launcher",
    "productivity",
    "spotlight"
  ],
  "private": true,
  "scripts": {
    "start": "npm run start:macos",
    "test": "jest",
    "lint": "eslint .",
    "install:macos": "cd ./macos && pod install",
    "start:macos": "node node_modules/react-native-macos/local-cli/cli.js run-macos",
    "build:macos": "node node_modules/react-native-macos/local-cli/cli.js run-macos --configuration Release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@types/math-expression-evaluator": "^1.2.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-macos": "^0.62.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.49",
    "@types/react-native": "^0.63.15",
    "@types/react-test-renderer": "^16.9.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
